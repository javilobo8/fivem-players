{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "no-console": false,
    "trailing-comma": [true, {"multiline": {"functions": "never"}}]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "./**/*.json"
    ]
  }
}
